{
  "name": "sanity-plugin-woodwing-assets",
  "version": "1.0.0",
  "description": "Search, preview and persist metadata from Woodwing Assets DAM",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/GyldendalDigital/kobber#readme",
  "bugs": {
    "url": "https://github.com/GyldendalDigital/kobber/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/GyldendalDigital/kobber.git"
  },
  "license": "MIT",
  "author": "Gyldendal",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "sanityPlugin": {
    "verifyPackage": {
      "eslintImports": false
    }
  },
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/ui": "^1.9.3"
  },
  "devDependencies": {
    "@gyldendal/kobber-eslint": "*",
    "@sanity/pkg-utils": "^7.2.0",
    "@sanity/plugin-kit": "^4.0.19",
    "@types/react": "^19.0.12",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sanity": "^3.80.1",
    "styled-components": "^6.1.16",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "react": ">=18",
    "sanity": ">=3"
  },
  "engines": {
    "node": ">=18"
  }
}
