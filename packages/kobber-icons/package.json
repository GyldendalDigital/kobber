{
  "name": "@gyldendal/kobber-icons",
  "version": "0.6.4",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    "./react": "./dist/react/index.js",
    "./web-components": "./dist/web-components/index.js",
    "./symbols/*.svg": "./dist/symbols/*.svg",
    "./symbols/*.ts": "./dist/symbols/*.ts",
    "./init": "./dist/init/index.js"
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules dist src/tmp",
    "lint": "yarn lint:js",
    "lint:js": "echo \"No JS linting configured\"",
    "prepare": "npm run build",
    "dev": "onchange \"./src/**/*.{ts,tsx,css}\" -- npm run build",
    "test": "node ./src/icons.test.js",
    "svg": "rm -rf dist src/tmp && svg-sprite --config ./svg-scripts/svg-sprite-config.json src/assets/svgs/**/*.svg",
    "build": "yarn svg && tsup"
  },
  "dependencies": {
    "@gyldendal/kobber-base": "0.6.4",
    "lit": "^3.2.1"
  },
  "devDependencies": {
    "@gyldendal/kobber-stylelint": "0.6.4",
    "@gyldendal/kobber-tsconfig": "0.6.4",
    "@lit/react": "^1.0.7",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^19.1.0",
    "jsdom": "^24.1.0",
    "onchange": "^7.1.0",
    "react": "^19.1.0",
    "stylelint": "^16.0.2",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-order": "^6.0.4",
    "svg-sprite": "^2.0.4",
    "tsup": "^8.4.0",
    "typedoc": "^0.28.10",
    "typedoc-github-wiki-theme": "^2.1.0",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@lit/react": ">=1",
    "react": ">=18"
  },
  "peerDependenciesMeta": {
    "@lit/react": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GyldendalDigital/kobber"
  }
}
