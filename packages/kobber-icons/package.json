{
  "name": "@gyldendal/kobber-icons",
  "version": "0.3.14",
  "license": "MIT",
  "type": "module",
  "files": [
    "chunks/**/*",
    "react/**/*",
    "symbols/**/*",
    "web-components/**/*"
  ],
  "scripts": {
    "lint": "yarn lint:js & yarn lint:css",
    "lint:js": "eslint ./src/**/*.{ts,tsx}",
    "lint:css": "stylelint ./src/**/*.css",
    "prepare": "npm run build",
    "dev": "onchange \"./src/**/*.{ts,tsx,css}\" -- npm run build",
    "test": "web-test-runner",
    "svg": "svg-sprite --config svg-sprite-config.json src/assets/svgs/**/*.svg",
    "build": "yarn svg && tsup"
  },
  "dependencies": {
    "@gyldendal/kobber-base": "*",
    "@lit/react": "^1.0.2",
    "@r2wc/react-to-web-component": "^2.0.3",
    "lit": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@gyldendal/kobber-eslint": "*",
    "@gyldendal/kobber-stylelint": "*",
    "@types/react": "18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@web/dev-server-esbuild": "^1.0.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^1.11.1",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-mdx": "^2.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jsdom": "^24.1.0",
    "onchange": "^7.1.0",
    "prettier": "^3.1.0",
    "stylelint": "^16.0.2",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-order": "^6.0.4",
    "svg-sprite": "^2.0.4",
    "tsup": "^7.2.0",
    "typedoc": "^0.25.12",
    "typedoc-github-wiki-theme": "^1.1.0",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GyldendalDigital/kobber"
  }
}
