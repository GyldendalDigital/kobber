{
  "name": "@gyldendal/kobber-icons",
  "version": "0.6.13",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    "./react": "./dist/react/index.js",
    "./web-components": "./dist/web-components/index.js",
    "./symbols/*.svg": "./dist/symbols/*.svg",
    "./symbols/*.ts": "./dist/symbols/*.ts",
    "./init": "./dist/init/index.js"
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules dist src/tmp",
    "prepare": "npm run build",
    "dev": "onchange \"./src/**/*.{ts,tsx,css}\" -- npm run build",
    "svg": "rm -rf dist src/tmp && svg-sprite --config ./svg-scripts/svg-sprite-config.json src/assets/svgs/**/*.svg",
    "build": "yarn svg && tsup",
    "create-ts-refs": "create-ts-refs --sourceMap=entries.ts --out=.references.mts"
  },
  "dependencies": {
    "@gyldendal/kobber-base": "0.6.13",
    "lit": "^3.2.1"
  },
  "devDependencies": {
    "@gyldendal/kobber-tsconfig": "0.6.13",
    "@internal/integration-checker": "workspace:^",
    "@lit/react": "^1.0.7",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^19.1.0",
    "jsdom": "^24.1.0",
    "onchange": "^7.1.0",
    "react": "^19.1.0",
    "svg-sprite": "^2.0.4",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@lit/react": ">=1",
    "react": ">=18"
  },
  "peerDependenciesMeta": {
    "@lit/react": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GyldendalDigital/kobber"
  }
}
