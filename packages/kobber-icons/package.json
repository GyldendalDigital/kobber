{
  "name": "@gyldendal/kobber-icons",
  "version": "0.3.66",
  "license": "MIT",
  "type": "module",
  "files": [
    "chunks/**/*",
    "react/**/*",
    "symbols/**/*",
    "web-components/**/*"
  ],
  "scripts": {
    "lint": "yarn lint:js & yarn lint:css",
    "lint:js": "eslint ./src/**/*.{ts,tsx}",
    "lint:css": "stylelint ./src/**/*.css",
    "prepare": "npm run build",
    "dev": "onchange \"./src/**/*.{ts,tsx,css}\" -- npm run build",
    "test": "node ./src/icons.test.js",
    "svg": "svg-sprite --config svg-sprite-config.json src/assets/svgs/**/*.svg",
    "build": "yarn svg && tsup"
  },
  "dependencies": {
    "@gyldendal/kobber-base": "*",
    "lit": "^3.2.1"
  },
  "devDependencies": {
    "@gyldendal/kobber-eslint": "*",
    "@gyldendal/kobber-stylelint": "*",
    "@lit/react": "^1.0.7",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-mdx": "^2.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jsdom": "^24.1.0",
    "onchange": "^7.1.0",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "stylelint": "^16.0.2",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-order": "^6.0.4",
    "svg-sprite": "^2.0.4",
    "tsup": "^7.2.0",
    "typedoc": "^0.27.6",
    "typedoc-github-wiki-theme": "^2.1.0",
    "typedoc-plugin-markdown": "^4.4.1",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@lit/react": ">=1",
    "react": ">=18"
  },
  "peerDependenciesMeta": {
    "@lit/react": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GyldendalDigital/kobber"
  }
}
