---
import { fetchCmsData } from "../api/cms.api";
import Layout from "../layouts/Layout.astro";
const data = await fetchCmsData();
---

<Layout title="Bloggen">
  
<h1>Heia bloggen!</h1>

<ul>
  {
    data.data.documents.map((d: any) => (
      <li>
        {d.title} - {d.author}
      </li>
    ))
  }
</ul>

<pre>{JSON.stringify(data, null, 2)}</pre>

</Layout>
